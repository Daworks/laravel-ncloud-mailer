{
  "name": "daworks/ncloud-cloud-outbound-mailer",
  "description": "Ncloud Cloud Outbound Mailer driver for Laravel",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.2",
    "symfony/mailer": "^v7.1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Daworks\\NcloudCloudOutboundMailer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daworks\\NcloudCloudOutboundMailer\\NcloudCloudOutboundMailerServiceProvider"
      ]
    }
  },
  "license": "MIT"
}
