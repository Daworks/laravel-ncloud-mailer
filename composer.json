{
  "name": "daworks/ncloud-cloud-outbound-mailer",
  "description": "Ncloud Cloud Outbound Mailer driver for Laravel",
  "type": "library",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "guzzlehttp/guzzle": "^7.2",
    "symfony/mailer": "^6.4.13",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Daworks\\NcloudCloudOutboundMailer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Daworks\\NcloudCloudOutboundMailer\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Daworks\\NcloudCloudOutboundMailer\\NcloudCloudOutboundMailerServiceProvider"
      ]
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Lee Deok Hyoung",
      "email": "dhlee@daworks.io"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
